<div class="nested-fields form-inline">
  <div class="form-group">
    <%= f.label :曲順 %>
    <%= f.number_field :song_number ,min:1,max:1000 %>
    <%= f.label :曲名 %>
    <%= f.text_field :name %>
    <%= f.label :アーティスト名 %>
    <%= f.select :artist_id, @artist.map{|a| [a.name,a.id]} %>
    <%= link_to_remove_association "曲を削除する", f %>
  </div>
</div>